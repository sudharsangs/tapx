{
  "manifest_version": 3,
  "name": "TapX",
  "version": "1.0.0",
   "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["build/injection.js"]
    }
  ],
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "scripting"
  ]
}